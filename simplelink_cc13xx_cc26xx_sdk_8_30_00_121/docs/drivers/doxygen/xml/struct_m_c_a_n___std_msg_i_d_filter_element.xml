      <memberdef kind="variable" id="struct_m_c_a_n___std_msg_i_d_filter_element_1a504660d72a30a2afa1cfa159576b0d81" prot="public" static="no" mutable="no">
        <type>uint32_t</type>
        <definition>uint32_t MCAN_StdMsgIDFilterElement::sft</definition>
        <argsstring></argsstring>
        <name>sft</name>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
<para>Standard Filter Type (2-bits) <linebreak/>
 0 = Range filter from SFID1 to SFID2 (SFID2 &gt;= SFID1) <linebreak/>
 1 = Dual ID filter for SFID1 or SFID2 <linebreak/>
 2 = Classic filter: SFID1 = filter, SFID2 = mask <linebreak/>
 3 = Filter element disabled </para>        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
        <location file="source/third_party/mcan/MCAN.h" line="1174" column="1" bodyfile="source/third_party/mcan/MCAN.h" bodystart="1174" bodyend="-1"/>
