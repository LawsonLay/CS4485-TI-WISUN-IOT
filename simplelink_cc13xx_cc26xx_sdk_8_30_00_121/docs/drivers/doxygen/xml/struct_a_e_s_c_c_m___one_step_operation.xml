        <argsstring></argsstring>
        <name>aad</name>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
<para>A buffer of length <computeroutput>aadLength</computeroutput> containing additional authentication data to be authenticated/verified but not encrypted/decrypted. </para>        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
        <location file="source/ti/drivers/AESCCM.h" line="953" column="1" bodyfile="source/ti/drivers/AESCCM.h" bodystart="953" bodyend="-1"/>
