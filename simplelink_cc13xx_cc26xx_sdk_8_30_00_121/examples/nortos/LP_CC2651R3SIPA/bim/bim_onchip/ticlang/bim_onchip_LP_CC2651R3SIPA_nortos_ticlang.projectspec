        </file>
        <file path="${COM_TI_SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR}/source/ti/common/cc26xx/ecc/sign_util.c" openOnCreation="false" excludeFromBuild="false" action="link" targetDirectory="Application" applicableConfigurations="Debug, Release">
        </file>
        <file path="${COM_TI_SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR}/source/ti/common/cc26xx/ecc/sign_util.h" openOnCreation="false" excludeFromBuild="false" action="link" targetDirectory="Application" applicableConfigurations="Debug, Release">
        </file>
    </project>
