        </file>
        <file path="${COM_TI_SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR}/source/ti/common/cc26xx/oad/oad_image_header.h" openOnCreation="false" excludeFromBuild="false" action="link" targetDirectory="Application">
        </file>
