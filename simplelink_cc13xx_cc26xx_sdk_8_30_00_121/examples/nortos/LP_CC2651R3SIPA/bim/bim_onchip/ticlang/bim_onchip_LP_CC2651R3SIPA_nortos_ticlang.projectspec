        </file>
        <file path="${COM_TI_SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR}/source/ti/common/flash/no_rtos/extFlash/bsp_spi.h" openOnCreation="false" excludeFromBuild="false" action="link" targetDirectory="SPI">
